Root: HKLM; SubKey: "Software\Microsoft\Windows\CurrentVersion\App Paths\WinEQ.exe"; ValueType: string; ValueName: "Path"; ValueData: "{app}"; Flags: uninsdeletevalue
Root: HKCU; Subkey: "Software\WinEQ"; Flags: uninsdeletekey
Root: HKCU; Subkey: "Software\WinEQ\1.0"; Flags: uninsdeletekey
